{
  "name": "frappe-gantt-react-wrapper",
  "version": "0.1.1",
  "description": "A minimal React wrapper for frappe-gantt",
  "main": "dist/frappe-gantt-react-wrapper.umd.js",
  "module": "dist/frappe-gantt-react-wrapper.es.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/frappe-gantt-react-wrapper.es.js",
      "require": "./dist/frappe-gantt-react-wrapper.umd.js"
    }
  },
  "files": ["dist", "src"],
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc -p tsconfig.build.json",
    "lint": "biome lint . --fix",
    "lint:no-fix": "biome lint .",
    "format": "biome format . --write",
    "format:no-write": "biome format .",
    "preview": "vite preview"
  },
  "dependencies": {
    "frappe-gantt": "^1.0.3"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@eslint/js": "^9.21.0",
    "@types/frappe-gantt": "^0.9.0",
    "@types/node": "^22.13.14",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "globals": "^15.15.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mickamy/frappe-gantt-react-wrapper.git"
  },
  "keywords": [
    "react",
    "gantt",
    "frappe-gantt",
    "project-management",
    "timeline",
    "gantt-chart",
    "scheduling"
  ],
  "author": "Tetsuro Mikami <tetsuro@mickamy.com>",
  "license": "MIT"
}
